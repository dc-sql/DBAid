<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <clear/>
    <!-- name = instance name. Examples: Server1 > name="MSSQLSERVER"; Server2\inst1 > name="INST1" -->
    <add name="SQL2012" connectionString="Data Source=.\SQL2012;Initial Catalog=_dbaid;Integrated Security=True;"/>
    <add name="SQL2017" connectionString="Data Source=.\SQL2017;Initial Catalog=_dbaid;Integrated Security=True;"/>
  </connectionStrings>
  <appSettings>
    <!-- "cache_" followed by procedure name allows you to spool check data to reduce overhead. value in seconds -->
    <add key ="cache_chart_capacity_fg" value="300"/>
    <add key ="cache_check_backup" value="900"/>
    <add key ="cache_check_integrity" value="7200"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v2.0.50727"/>
    <supportedRuntime version="v4.0"/>
  </startup>
</configuration>
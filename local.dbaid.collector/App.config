<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <connectionStrings>
    <clear />
    <add name="ServerName" connectionString="Server=SeverName;Database=_dbaid;Trusted_Connection=True;" />
  </connectionStrings>
  <appSettings>
    <add key="WorkingDirectory" value="C:\Temp\DBAid-Collection" />
    <add key="ProcessedFileRetentionDays" value="0"/>
    <add key="EmailEnable" value="true" />
    <add key="EmailSmtp" value="smtp.domain.co.nz" />
    <add key="EmailTo" value="person@domain.co.nz" />
    <add key="EmailFrom" value="person@domain.co.nz" />
    <add key="EmailSubject" value="Test DBAid C# App" />
    <add key="EmailAttachmentByteLimit" value="10000000" />
    <add key="EmailAttachmentCountLimit" value="15" />
    <add key="EmailEnableSsl" value="true" />
    <add key="EmailIgnoreSslError" value="false" />
    <add key="EmailAnonymous" value="false" />
    <add key="logVerbose" value="true"/>
    <add key="default_cmd_timeout_sec" value="30" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" />
    <supportedRuntime version="v2.0.50727"/>
  </startup>
</configuration>
